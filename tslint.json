{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "**/node_modules/**",
      "**/generated/**"
    ]
  },
  "rules": {
    "no-boolean-literal-compare": false,
    "max-line-length": false,
    "import-name": false
  }
}